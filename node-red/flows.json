    {
        "id": "ca96aef36daaf23f",
        "type": "function",
        "z": "488166910467e566",
        "name": "Parser Compteurs",
        "func": "var parts = msg.payload.split(',');\nvar compteurTemp = parseInt(parts[0]) || 0;\nvar compteurHum = parseInt(parts[1]) || 0;\nvar compteurCrit = parseInt(parts[2]) || 0;\n\n// Envoyer aux compteurs\nreturn [\n    {payload: compteurTemp},\n    {payload: compteurHum},\n    {payload: compteurCrit}\n];",
        "outputs": 3,
