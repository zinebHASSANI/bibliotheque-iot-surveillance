    {
        "id": "b11859b864883d57",
        "type": "function",
        "z": "488166910467e566",
        "name": "Parser Temp√©rature",
        "func": "var temp = parseFloat(msg.payload);\nmsg.payload = temp;\n\n// Stocker dans le contexte global pour CSV\nvar data = global.get('sensor_data') || [];\ndata.push({\n    timestamp: new Date().toISOString(),\n    temperature: temp,\n    humidite: null,\n    status: null\n});\nif (data.length > 1000) data.shift();\nglobal.set('sensor_data', data);\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 370,
        "y": 100,
        "wires": [
            [
                "e560898dee7112e8",
                "1cb68b2754ca2506",
                "b3ba1aa736dbddac"
