    {
        "id": "0f2bfae40cd042a8",
        "type": "function",
        "z": "488166910467e566",
        "name": "Parser Humidit√©",
        "func": "var hum = parseFloat(msg.payload);\nmsg.payload = hum;\n\n// Stocker dans le contexte global\nvar data = global.get('sensor_data') || [];\nif (data.length > 0) {\n    data[data.length - 1].humidite = hum;\n}\n\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 360,
